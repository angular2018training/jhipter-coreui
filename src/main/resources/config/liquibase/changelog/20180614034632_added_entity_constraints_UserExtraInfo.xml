<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity UserExtraInfo.
    -->
    <changeSet id="20180614034632-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_extra_infos_id"
                                 baseTableName="user_extra_info_role"
                                 constraintName="fk_user_extra_info_role_user_extra_infos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra_info"/>
        <addForeignKeyConstraint baseColumnNames="roles_id"
                                 baseTableName="user_extra_info_role"
                                 constraintName="fk_user_extra_info_role_roles_id"
                                 referencedColumnNames="id"
                                 referencedTableName="role"/>
        
        <addForeignKeyConstraint baseColumnNames="user_extra_infos_id"
                                 baseTableName="user_extra_info_user_group"
                                 constraintName="fk_user_extra_info_user_group_user_extra_infos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra_info"/>
        <addForeignKeyConstraint baseColumnNames="user_groups_id"
                                 baseTableName="user_extra_info_user_group"
                                 constraintName="fk_user_extra_info_user_group_user_groups_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_group"/>
        
    </changeSet>
</databaseChangeLog>
