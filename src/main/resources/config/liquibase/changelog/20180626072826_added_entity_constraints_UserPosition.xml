<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity UserPosition.
    -->
    <changeSet id="20180626072826-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="company_id"
                                 baseTableName="user_position"
                                 constraintName="fk_user_position_company_id"
                                 referencedColumnNames="id"
                                 referencedTableName="company"/>

        <addForeignKeyConstraint baseColumnNames="post_office_id"
                                 baseTableName="user_position"
                                 constraintName="fk_user_position_post_office_id"
                                 referencedColumnNames="id"
                                 referencedTableName="post_office"/>

        <addForeignKeyConstraint baseColumnNames="position_id"
                                 baseTableName="user_position"
                                 constraintName="fk_user_position_position_id"
                                 referencedColumnNames="id"
                                 referencedTableName="position"/>

        <addForeignKeyConstraint baseColumnNames="user_positions_id"
                                 baseTableName="user_position_user_group"
                                 constraintName="fk_user_position_user_group_user_positions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_position"/>
        <addForeignKeyConstraint baseColumnNames="user_groups_id"
                                 baseTableName="user_position_user_group"
                                 constraintName="fk_user_position_user_group_user_groups_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_group"/>
        
        <addForeignKeyConstraint baseColumnNames="user_extra_info_id"
                                 baseTableName="user_position"
                                 constraintName="fk_user_position_user_extra_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_extra_info"/>

    </changeSet>
</databaseChangeLog>
